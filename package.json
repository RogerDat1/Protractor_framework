    "testing",
    "behaviour driven development",
    "bdd",
    "selenium"
  ],
  "dependencies": {
    "await": "0.2.6",
    "chai": "^4.1.0",
    "chai-as-promised": "^7.1.1",
    "cross-env": "^5.1.3",
    "cucumber": "^2.3.0",
    "cucumber-html-report": "^0.6.0",
    "cucumber-html-reporter": "^2.0.2",
    "exceljs": "^0.4.2",

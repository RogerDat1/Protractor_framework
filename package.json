    "name": "Amod Mahajan",
    "email": "amod.mahajan@mercer.com"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "sampletest": "((SET TEST_ENV=QA)&&(protractor --cucumberOpts.tags=@Smoke))",
    "regression": "webdriver-manager update && protractor ./protractor.conf.js --cucumberOpts.tags=@Regression",
    "smokeTesting": "webdriver-manager update && protractor ./protractor.conf.js --cucumberOpts.tags=@Smoke"
  },
  "repository": {
